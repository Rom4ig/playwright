{
  "root": true,
  "parser": "@typescript-eslint/parser",
  "plugins": [
    "@typescript-eslint"
  ],
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/eslint-recommended",
    "plugin:@typescript-eslint/recommended"
  ],
  "rules": {
    "quotes": 2,
    "prefer-const": 2,
    "semi": 2,
    "no-unused-vars": 2,
    "object-curly-spacing": [
      "error",
      "always"
    ]
  }
}